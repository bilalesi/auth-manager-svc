APP_NAME=Auth Manager Service
VERSION=1.0.0
DEBUG=false
LOG_LEVEL=INFO
PORT=8000


DATABASE_URL=postgresql+asyncpg://postgres:postgres@localhost:5432/auth_manager
DATABASE_POOL_SIZE=10
DATABASE_MAX_OVERFLOW=20
DATABASE_POOL_TIMEOUT=30
DATABASE_ECHO=false


KEYCLOAK_ISSUER=http://localhost:8080/auth
KEYCLOAK_CLIENT_ID=auth
KEYCLOAK_CLIENT_SECRET=your-client-secret-here
KEYCLOAK_REALM=SBO
KEYCLOAK_REDIRECT_URI=http://localhost:8000/v1/offline-token/callback
KEYCLOAK_AFTER_CONSENT_REDIRECT_URI=http://localhost:3000/app/consent-accepted

# Generate with: openssl rand -hex 32
AUTH_MANAGER_TOKEN_VAULT_ENCRYPTION_KEY=0123456789abcdef0123456789abcdef0123456789abcdef0123456789abcdef

# Generate with: openssl rand -base64 32
STATE_TOKEN_SECRET=your-state-token-secret-here
STATE_TOKEN_EXPIRY=3600

CORS_ORIGINS=http://localhost:3000,http://localhost:8000
CORS_ALLOW_CREDENTIALS=true
